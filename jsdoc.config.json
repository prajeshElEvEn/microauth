{
  "source": {
    "include": ["src"],
    "exclude": ["node_modules"]
  },
  "opts": {
    "destination": "./docusaur/docs",
    "recurse": true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "optsType": {
    "param": " — ",
    "property": " — "
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "markdown": {
    "parser": "gfm"
  },
  "recurseDepth": 10,
  "sourceType": "module",
  "encoding": "utf8",
  "readme": "README.md",
  "package": "package.json"
}
